<!doctype html>
<html lang="en">

  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.1.1/dist/css/bootstrap.min.css" integrity="sha384-WskhaSGFgHYWDcbwN70/dfYBj47jz9qbsMId/iRN3ewGhXQFZCSftd1LZCfmhktB" crossorigin="anonymous">
    <title>Free SS</title>
  </head>


  <body>
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
      <a class="navbar-brand" href="https://github.com/arcnavier/ss">Shadowsocks</a>
      <ul class="navbar-nav mr-auto">
        <li class="nav-item active">
          <a class="nav-link" href="https://github.com/arcnavier/ss">GitHub</a>
        </li>
      </ul>
    </nav>

    <div class="container">

      <div class="mt-3">
        <p>Source: <a href="https://fast.ishadowx.net/">https://fast.ishadowx.net/</a>
      </div>

      <div>
        <p>Updated: {{ update }}</p>
      </div>

      <div class="card">
        <div class="card-body">
          <p>ShadowsocksR Subscribe:</p>
          <code>https://arcnavier.github.io/ss/ssr.txt</code>
        </div>
      </div>

      {{#data}}
      <div class="card">
        <h5 class="card-header">{{ name }}</h5>
        <div class="card-body">
          <pre id="target{{id}}">{{{encodecmd}}}</pre>
          <a class="card-link" href="#" data-toggle="collapse" data-target="#qrcode{{id}}">二维码</a>
          <a class="card-link" href="{{{encodess}}}">一键配置</a>
          <a class="card-link noclick" href="#" data-clipboard-target="#target{{id}}">复制命令</a>
        </div>

        <div class="collapse card-body" id="qrcode{{id}}">
          <p><a href="{{{encodess}}}">{{encodess}}</a></p>
          <img src="{{{qrcode}}}" />
        </div>
      </div>
      {{/data}}

      {{#datassr}}
      <div class="card">
        <h5 class="card-header">{{ name }}</h5>
        <div class="card-body">
          <pre id="target{{id}}">{{{encodecmd}}}</pre>
          <a class="card-link" href="{{{encodess}}}">一键配置</a>
          <a class="card-link noclick" href="#" data-clipboard-target="#target{{id}}">复制命令</a>
        </div>
      </div>
      {{/datassr}}


    </div>

    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.3.1/dist/jquery.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.14.3/dist/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.1.1/dist/js/bootstrap.min.js" integrity="sha384-smHYKdLADwkXOn1EmN1qk/HfnUcbVRZyYmZ4qpPea6sjB/pTJ0euyQp0Mk8ck+5T" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/clipboard@2.0.1/dist/clipboard.min.js"></script>
    <script>
      new ClipboardJS('.card-link');
      $('.noclick').click(function(e){
        e.preventDefault();
      });
    </script>
  </body>
</html>
